    <script>
      // JavaScript to toggle accordions
      document.querySelectorAll('.cs_accordian_head').forEach(function(accordionHead) {
        accordionHead.addEventListener('click', function() {
          const parent = this.closest('.cs_accordian');
          const isActive = parent.classList.contains('active');
    
          // Close all accordions
          document.querySelectorAll('.cs_accordian').forEach(function(accordion) {
            accordion.classList.remove('active');
          });
    
          // Open the clicked accordion if it was not already open
          if (!isActive) {
            parent.classList.add('active');
          }
        });
      });
    </script>
